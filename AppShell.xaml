<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp5.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp5.Pages"
    Shell.FlyoutBehavior="Flyout">

    <FlyoutItem FlyoutDisplayOptions="AsMultipleItems">
        <ShellContent
        Title="League of Legends"
        ContentTemplate="{DataTemplate local:LolPage}"
        FlyoutIcon="lol_img.png"
         /> 

    <ShellContent
        Title="Valorant"
        ContentTemplate="{DataTemplate local:ValorantPage}"
        FlyoutIcon="valorant_img.png"
         />
    <ShellContent
        Title="Teamfight Tactics"
        ContentTemplate="{DataTemplate local:TFTPage}"
        FlyoutIcon="tft_img.png"
         />
    </FlyoutItem>

</Shell>
